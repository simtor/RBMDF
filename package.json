  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "express": "^4.17.1",
    "pug": "^3.0.0"
  }
}
