{
  "name": "rbmdf",
  "version": "1.0.0",
  "description": "Medical Drug Finder",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "copy-styles": "copyfiles -f \"src/views/*.pug\" out/views",
